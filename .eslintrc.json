{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "eslint:recommended", "plugin:jest/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-syntax": [
            "error",
            "LabeledStatement",
            "WithStatement"
            ]
    },
    "plugins": ["jest"]
}


